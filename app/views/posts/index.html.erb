<html>

<head>
</head>

<body>

  <h1>allposts</h1>

  <div>
    <ul>
      <% @posts.each do |post| %>
        <li>
          <%= link_to post.title, post_path(post) %>
          <%= link_to "edit", edit_post_path(post) %>
          <%= link_to "delete", post_path(post), method: :delete %>
        </li>
      <% end %>
    </ul>
  </div>

</body>




</html>
